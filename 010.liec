def int_list_len (l : [int]) -> int :
    if int_is_empty(l):
        return 0
    else:
        return 1 + int_list_len(int_tail(l))

def int_list_sum (l : [int]) -> int :
    if int_is_empty(l):
        return 0
    else:
        return int_head(l) + int_list_sum(int_tail(l))

